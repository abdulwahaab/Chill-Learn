        <div class="card ps-container col-md-12">
            <div class="table-responsive no-wrap">
                <table class="table vm no-th-brd pro-of-month manage-admin-list payment-text">
                    <tbody>
                        @if (Model != null && Model.Count() > 0)
                        {
                            foreach (var item in Model)
                            {
                                <tr>
                                    <td style="width:50px;"><span class="round round-img round-img-block"><img src="/content/images/@item.Picture" alt="user" style="width:100%;height:100%;"></span></td>
                                    <td>
                                        <h6>@item.FirstName @item.LastName </h6><small class="text-muted">Date </small><p><span>@item.CreationDate</span></p>
                                    </td>
                                    <td class="text-right"><a href="/admin/detail?id=@item.UserID" class="btn btn-warning pending-but text-white">View</a></td>
                                </tr>
                            }
                        }
                    </tbody>
                </table>
            </div>
        </div>
    </div>

