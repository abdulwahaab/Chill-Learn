@model IEnumerable<ChillLearn.CustomModels.ClassesModel>
@using ChillLearn.Enums;
@{
    ViewBag.Title = "Classes";
    Layout = "~/Views/Shared/TeacherLayout.cshtml";
}
<div class="row page-titles m-b-10">

    <div class="col-md-5 align-self-center manage-block-left">
        <h3 class="inbox-text manage-text">Manage Classes</h3>
    </div>
    <div class="col-md-7 align-self-center list-sort text-right delete-message today-text-heading">
        <a href="~/class/create" class="btn btn-rounded btn-outline-info">Create New Class</a>
    </div>

</div>
<div class="row">
    <div class="col-md-12 p-l-0 p-r-0 m-t-20">
        <div class="card card-bg">
            <div class="card-body p-l-0 p-r-0 p-t-0 p-b-0">
                <!-- Nav tabs -->
                <ul class="nav nav-tabs" role="tablist">
                    <li class="nav-item"> <a class="nav-link active" data-toggle="tab" href="#upcoming" role="tab"> <span class="hidden-xs-down">Upcoming Classes</span></a> </li>
                    <li class="nav-item"> <a class="nav-link" data-toggle="tab" href="#past" role="tab"> <span class="hidden-xs-down">Past Classes</span></a> </li>
                    <li class="nav-item"> <a class="nav-link" data-toggle="tab" href="#cancelled" role="tab"> <span class="hidden-xs-down">Cancelled Classes</span></a> </li>
                    <li class="nav-item"> <a class="nav-link" data-toggle="tab" href="#Recorded" role="tab"> <span class="hidden-xs-down">Recorded Classes</span></a> </li>
                </ul>
                <!-- Tab panes -->
                <div class="tab-content tabcontent-border">
                    <div class="tab-pane active" id="upcoming" role="tabpanel">

                        <!-- tab-box1 -->
                        <div class="p-10 bg-white m-t-30 radius-box">
                            <div class="table-responsive table-cell-box no-wrap">
                                <table class="table vm no-th-brd pro-of-month manage-admin-list student-classes m-b-0">
                                    <tbody>
                                        @if (Model != null && Model.Count() > 0)
                                        {
                                            foreach (var cls in Model.Where(a => a.ClassDate > DateTime.Now))
                                            {
                                        <tr>
                                            @{
                                                var enumDisplayStatus = (SessionType)@cls.SessionType;
                                                string sessionType = enumDisplayStatus.ToString();
                                            }
                                            <td style="width:50px;"><span class="round round-student"><img src="~/Content/teacher/images/user-large-student.png" alt="user" width="50"></span></td>
                                            <td>
                                                <h6>@cls.Title <span><img src="~/Content/teacher/images/info.svg"></span></h6><small class="text-muted">Date &amp; Time : @cls.ClassDate.ToString("dd MMM yyyy"), @cls.ClassTime</small><ul class="subject-list">
                                                    <li>Subject : @cls.SubjectName</li>
                                                    <li>Duration: @cls.Duration hrs</li>
                                                    <li>Session: @sessionType</li>
                                                </ul>
                                            </td>

                                            <td class="text-center dropdown">
                                                <a href="javascript:;" data-toggle="dropdown" data-hover="dropdown"><img src="~/Content/teacher/images/dotted-line.png"></a>
                                                <div class="dropdown-menu dropdown-menu-right right-dropdown">
                                                    <a class="dropdown-item" href="javascript:;"><b>Actions</b></a>
                                                    <a class="dropdown-item" href="javascript:;"><img src="~/Content/teacher/images/play-button.svg">Start Class</a>
                                                    <a class="dropdown-item" href="javascript:;" data-toggle="modal" data-target="#myModal"><img src="~/Content/teacher/images/error.svg">Cancel Class</a>
                                                    @*<a class="dropdown-item" href="javascript:;" data-toggle="modal" data-target="#myModal2"><img src="~/Content/teacher/images/history-clock-button.svg">Reschedule Class</a>*@
                                                    <a class="dropdown-item" href="/tutor/requests?c=@cls.ClassId"><img src="~/Content/teacher/images/history-clock-button.svg">Requests</a>
                                                </div>
                                            </td>

                                        </tr>
                                            }
                                        }
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
